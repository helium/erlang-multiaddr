{plugins, [coveralls, rebar3_hex]}.
{cover_enabled, true}.
{cover_export_enabled, true}.
{coveralls_coverdata, "_build/test/cover/eunit.coverdata"}.
{coveralls_service_name, "travis-ci"}.

{erl_opts, [warn_untyped_records, debug_info, warnings_as_errors]}.

{deps, [
        {erl_base58, "0.0.1"},
        {base32, "0.1.0"},
        {small_ints, "0.1.0"}
       ]}.
